{
  "name": "json-quick-tools",
  "displayName": "JSON QuickTools",
  "description": "Instantly parse string into readable JSON or compactify JSON into a string, all with convenient keyboard shortcuts.",
  "repository": "https://github.com/sidhique/json-quick-tools",
  "publisher": "Sidhique",
  "version": "0.0.6",
  "icon": "images/icon.webp",
  "engines": {
    "vscode": "^1.0.0"
  },
  "categories": [
    "Other"
  ],
  "tags": [
    "json",
    "json tools",
    "format json",
    "parse json",
    "stringify json",
    "json formatter",
    "json parser",
    "json utilities",
    "developer tools",
    "productivity",
    "json helper",
    "json convert",
    "json prettify",
    "json compact",
    "json syntax"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "json-quick-tools.parseJson",
        "title": "Parse Text to JSON"
      },
      {
        "command": "json-quick-tools.stringify",
        "title": "Stringify JSON"
      }
    ],
    "keybindings": [
      {
        "command": "json-quick-tools.parseJson",
        "key": "cmd+shift+p",
        "win": "alt+win+p",
        "linux": "alt+win+p",
        "when": "editorTextFocus"
      },
      {
        "command": "json-quick-tools.stringify",
        "key": "cmd+shift+o",
        "win": "alt+win+o",
        "linux": "alt+win+o",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.0.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "eslint": "^9.25.1",
    "typescript": "^5.8.3",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2"
  }
}